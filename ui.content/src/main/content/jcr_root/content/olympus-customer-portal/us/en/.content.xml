<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        pageTitle="OCA customer portal"
        jcr:primaryType="cq:PageContent"
        jcr:title="en"
        cq:contextHubPath="/etc/cloudsettings/default/contexthub"
        cq:contextHubSegmentsPath="/etc/segmentation/contexthub"
        cq:template="/conf/olympus/settings/wcm/templates/page-content"
        cq:allowedTemplates="[/conf/olympus/settings/wcm/templates/(?!(landing|root)).*]"
        navRoot="{Boolean}true"
        sling:configRef="/conf/olympus/us/en"
        sling:resourceType="olympus/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="olympus/components/container"
            layout="responsiveGrid">
            <experiencefragment-header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="olympus/components/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/olympus/us/en/site/header/master"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="olympus/components/container"
                layout="responsiveGrid">
            </container>
            <experiencefragment-footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="olympus/components/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/olympus/us/en/site/footer/master"/>
        </root>
    </jcr:content>
</jcr:root>
