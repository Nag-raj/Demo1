<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Olympus Blue Header Navigation"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Properties"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <homeicon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                    allowUpload="{Boolean}false"
                                                    class="cq-droptarget"
                                                    fieldLabel="Home Icon"
                                                    fileNameParameter="./fileName"
                                                    fileReferenceParameter="./homeIcon"
                                                    mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                    name="./file"/>
                                            <homelabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                    fieldDescription="Text that represents Home "
                                                    fieldLabel="Home Label"
                                                    name="./homeLabel"
                                                    removeSingleParagraphContainer="{Boolean}true"
                                                    useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceSuperType="/apps/olympus/components/text/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text/rtePlugins"/>
                                                <uiSettings
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceSuperType="/apps/olympus/components/text/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text/uiSettings"/>
                                            </homelabel>
                                            <homeLinkPath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                    fieldDescription="Select the path for redirection"
                                                    fieldLabel="Home Link Path"
                                                    name="./homeLinkPath"
                                                    rootPath="/content/olympus-customer-portal/"/>
                                            <greetinglabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Text that represents Greeting like Welcome or Hello"
                                                    fieldLabel="Greeting Label"
                                                    name="./greetingLabel"/>
                                            <helpicon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                    allowUpload="{Boolean}false"
                                                    class="cq-droptarget"
                                                    fieldLabel="Help Icon"
                                                    fileNameParameter="./fileName"
                                                    fileReferenceParameter="./helpIcon"
                                                    mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                    name="./file"/>
                                            <helpLinkPath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                    fieldDescription="Select the path for redirection"
                                                    fieldLabel="Help Link Path"
                                                    name="./helpLinkPath"
                                                    rootPath="/content/olympus-customer-portal/"/>
                                            <carticon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                    allowUpload="{Boolean}false"
                                                    class="cq-droptarget"
                                                    fieldLabel="Cart Icon"
                                                    fileNameParameter="./fileName"
                                                    fileReferenceParameter="./cartIcon"
                                                    mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                    name="./file"/>
                                            <notificationicon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                    allowUpload="{Boolean}false"
                                                    class="cq-droptarget"
                                                    fieldLabel="Notification Icon"
                                                    fileNameParameter="./fileName"
                                                    fileReferenceParameter="./notificationIcon"
                                                    mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                    name="./file"/>
                                            <cartPagePath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                    fieldDescription="Select the path for cart page"
                                                    fieldLabel="Cart Page Path"
                                                    name="./cartPagePath"
                                                    rootPath="/content/olympus-customer-portal/"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <cartproperties
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Cart Drawer Properties"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <carttitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Text that represents Cart Title"
                                                    fieldLabel="Cart Drawer Title"
                                                    name="./cartTitle"/>
                                            <closeicon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                    allowUpload="{Boolean}false"
                                                    class="cq-droptarget"
                                                    fieldLabel="Close Icon"
                                                    fileNameParameter="./fileName"
                                                    fileReferenceParameter="./closeIcon"
                                                    mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                    name="./file"/>
                                            <carticoninoverlay
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                    allowUpload="{Boolean}false"
                                                    class="cq-droptarget"
                                                    fieldLabel="Cart Icon in Overlay"
                                                    fileNameParameter="./fileName"
                                                    fileReferenceParameter="./cartIconInOverlay"
                                                    mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                    name="./file"/>
                                            <subheading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Text that represents Sub-heading"
                                                    fieldLabel="Sub-Heading"
                                                    name="./subHeading"/>
                                            <buttonlabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Text that represents Button Label"
                                                    fieldLabel="Button Label"
                                                    name="./buttonLabel"/>

                                            <linkPath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                    fieldDescription="Select the path for redirection"
                                                    fieldLabel="Button Link Path"
                                                    name="./buttonLinkPath"
                                                    rootPath="/content/olympus-customer-portal/"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </cartproperties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>